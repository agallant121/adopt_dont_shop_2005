<h3><%= @pet.name %></h3>

 <%= image_tag(@pet.image) %>
<p>Age: <%= @pet.age %></p>
<p>Sex: <%= @pet.sex %></p>
<p>Description: <%= @pet.description %></p>
<p>Adopted? <%= @pet.adopted? %></p>
<p><%= link_to 'Update Pet', "/pets/#{@pet.id}/edit" %></p>
<p><%= link_to "Delete", "/pets/#{@pet.id}", method: :delete %></p>
